generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Session {
  id              String   @id @default(auto()) @db.ObjectId @map("_id")
  date            DateTime
  subjectType     String
  className       String
  timeEstimateH   Int
  startTime       String
  endTime         String
  sessionGoal     String
  productiveTimeH Int
  distractedTimeH Int
}
